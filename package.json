    "sircus-tools-layout": "^0.0.2",
    "sircus-tools-layout-responsive": "^0.0.3",
    "sircus-tools-overflow": "^0.0.2",
    "sircus-tools-overflow-responsive": "^0.0.3",
    "sircus-tools-space": "^0.0.2",
    "sircus-tools-space-responsive": "^0.0.3",
    "sircus-tools-typo": "^0.0.2",
    "sircus-tools-width": "^0.0.2",
    "sircus-tools-width-responsive": "^0.0.3"
  },
  "devDependencies": {
    "cssnext":"^1.7.1"
  },
  "scripts": {
    "css": "cssnext --no-color-rgba --no-rem ./src/sircus.css ./dist/sircus.css",
    "css:min": "cssnext --no-color-rgba --no-rem --compress ./src/sircus.css ./dist/sircus.min.css",
    "build": "npm run css && npm run css:min",
    "start": "npm install && npm run build"
