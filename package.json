  "description": "Flexible Modular CSS Family for Designing in the Browser.",
  "dependencies": {
    "sircus-components-button": "^2.0.0",
    "sircus-components-container": "1.0.0",
    "sircus-components-flex": "^1.0.0",
    "sircus-components-form": "^2.0.0",
    "sircus-components-fullembed": "^1.0.0",
    "sircus-components-grid": "1.0.0",
    "sircus-components-list": "1.0.0",
    "sircus-components-table": "^1.0.0",
    "sircus-elements": "^1.0.0",
    "sircus-tools-align": "^2.0.0",
    "sircus-tools-background": "^0.0.0",
    "sircus-tools-border": "^1.0.0",
    "sircus-tools-boxsizing": "^1.0.0",
    "sircus-tools-clear": "^1.0.0",
    "sircus-tools-display": "^2.0.0",
    "sircus-tools-gutter": "^2.0.0",
    "sircus-tools-height": "^0.0.0",
    "sircus-tools-layout": "^2.0.0",
    "sircus-tools-overflow": "^2.0.0",
    "sircus-tools-shadow": "^0.0.0",
    "sircus-tools-space": "^2.0.0",
    "sircus-tools-typography": "^2.0.0",
    "sircus-tools-width": "^2.0.0",
    "sircus-variables": "^1.0.0"
  },
  "devDependencies": {
    "csswring": "^5.0.0",
    "css-mqpacker": "^5.0.1",
    "postcss-banner": "^1.2.0",
    "postcss-calc": "^5.2.1",
    "postcss-cli": "^2.5.2",
    "postcss-color-function": "^2.0.0",
    "postcss-custom-media": "^5.0.1",
    "postcss-custom-properties": "^5.0.1",
    "postcss-import": "^8.1.2",
    "postcss-reporter": "^1.3.0"
  },
  "scripts": {
    "build": "mkdir -p css && npm run css && npm run css:minify",
    "css": "postcss -c postcss.js",
    "css:minify": "postcss -u csswring css/sircus.css -o css/sircus.min.css",
    "dev": "postcss -c postcss.js -w",
    "start": "npm install && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/sircus/sircus.git"
  },
  "homepage": "https://github.com/sircus/sircus",
  "author": {
    "name": "blivesta",
    "url": "http://blivesta.com"
  },
  "keywords": [
    "css",
    "design",
    "flexble",
    "modular",
    "scalable",
    "responsive",
    "rwd"
  ],
  "license": "MIT"
}
