{
  "name": "sircus",
  "version": "1.0.0-alpha.1",
  "description": "Modular CSS Family.",
  "homepage":"http://sircus.blivesta.com",
  "license":"MIT",
  "author": {
    "name": "Yasuyuki Enomoto",
